    "\n",
    "file_path = './320~'\n",
    "file_names = os.listdir(file_path)\n",
    "\n",
    "i = 320\n",
    "for name in file_names:\n",
    "    src = os.path.join(file_path, name)\n",
    "    dst = str(i) + '.jpg'\n",
    "    dst = os.path.join(file_path, dst)\n",
    "    os.rename(src, dst)\n",
    "    i += 1"
   ]
